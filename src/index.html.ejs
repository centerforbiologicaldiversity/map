<!doctype html>
<html lang="en">
<head>
  <title>Center for Biological Diversity - Attend an Event Near You</title>

  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <meta name="application-name" content="Ignite Change">
  <meta name="description" content="Turn your love for the wild into something powerful with the Center for Biological Diversity.">
  <meta property="og:image" content="https://s3.amazonaws.com/s3.biologicaldiversity.org/images/defaultmeta.png">
  <meta property="og:url" content="https://map.biologicaldiversity.org/">
  <meta property="og:title" content="Map: Attend an Event Near You">
  <meta property="og:description" content="Turn your love for the wild into something powerful with the Center for Biological Diversity.">
  <meta property="og:site_name" content="mobilize.BiologicalDiversity.org">
  <meta name="twitter:card" value="summary_large_image">
  <meta name="twitter:site" content="@CenterForBioDiv">
  <meta name="twitter:creator" content="@CenterForBioDiv">

  <link rel="shortcut icon" href="<%= htmlWebpackPlugin.files.favicon %>">

  <!-- Include style sheets generated by webpack -->
  <% for (var css in htmlWebpackPlugin.files.css) { %>
  <link href="<%= htmlWebpackPlugin.files.css[css] %>" rel="stylesheet">
  <% } %>
</head>

<body>
  <%= require('src/templates/OutdatedBrowser.html') %>
  <div id="app" class="layout">
    <div class="layout-header">
      <%= require('src/templates/Header.html') %>
    </div>
    <div class="layout-main">
      <%= require('src/templates/EventMap.html') %>
      <%= require('src/templates/EventList.html') %>
      <%= require('src/templates/Footer.html') %>
    </div>
  </div>
  <script src="<%= htmlWebpackPlugin.files.chunks['initialize'].entry %>"></script>
</body>
</html>
